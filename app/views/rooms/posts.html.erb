<h1>登録済みルーム一覧</h1>

<div class ="room-index">
  <% @rooms.each do |room| %>
   <% if current_user.id == room.user_id %>
   <div class ="room-frame">
     <%= link_to room_path(room) do %>
      <%= image_tag room.image.url, width: '300', height: 'auto' %><br>
     <% end %>
     <%= link_to room_path(room) do %>
      <%= room.title %><br>
     <% end %>
     住所:<%= room.address %><br>
     料金:¥<%= room.price %><br>
    </div>
   <% end %>
  <% end %>
</div>
